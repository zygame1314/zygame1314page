{
    "version": 2,
    "builds": [
        {
            "src": "**/*.{html,htm,css,js,jsx,ts,tsx,jpg,jpeg,png,gif,svg,ico,webp,mp3,mp4,wav,pdf,txt,json,xml,woff,woff2,ttf,eot,cur,crs,ani}",
            "use": "@vercel/static"
        },
        {
            "src": "api/**/*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1.js",
            "headers": {
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
                "Access-Control-Allow-Headers": "Content-Type",
                "X-Real-IP": "43.230.89.163,43.230.89.164,43.230.89.165,43.230.89.166,43.230.89.167",
                "X-Forwarded-For": "43.230.89.163,43.230.89.164,43.230.89.165,43.230.89.166,43.230.89.167"
            }
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}